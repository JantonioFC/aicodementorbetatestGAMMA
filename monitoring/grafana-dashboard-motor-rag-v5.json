{
  "dashboard": {
    "id": null,
    "title": "Motor RAG - v5.0",
    "tags": ["ai-code-mentor", "rag", "production", "education"],
    "style": "dark",
    "timezone": "browser",
    "refresh": "30s",
    "schemaVersion": 27,
    "version": 1,
    "description": "Dashboard de monitoreo para el Motor RAG v5.0 - Sistema de Generaci√≥n Aumentada por Recuperaci√≥n educativo",
    
    "panels": [
      {
        "id": 1,
        "title": "üìä M√©tricas Generales - Motor RAG v5.0",
        "type": "stat",
        "targets": [],
        "gridPos": {"h": 3, "w": 24, "x": 0, "y": 0},
        "options": {
          "textMode": "name",
          "colorMode": "background",
          "graphMode": "none"
        },
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "thresholds": {
              "steps": [
                {"color": "green", "value": null}
              ]
            }
          }
        }
      },
      
      {
        "id": 2,
        "title": "üö® Tasa de Errores (%)",
        "type": "stat",
        "targets": [
          {
            "expr": "rate(http_requests_total{job=\"ai-code-mentor\",endpoint=\"/api/generate-lesson\",status!~\"2..\"}[5m]) / rate(http_requests_total{job=\"ai-code-mentor\",endpoint=\"/api/generate-lesson\"}[5m]) * 100",
            "legendFormat": "Error Rate %",
            "refId": "A"
          }
        ],
        "gridPos": {"h": 8, "w": 6, "x": 0, "y": 3},
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "textMode": "auto",
          "colorMode": "background",
          "graphMode": "area",
          "justifyMode": "center"
        },
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 5},
                {"color": "red", "value": 10}
              ]
            },
            "unit": "percent",
            "min": 0,
            "max": 100
          }
        },
        "alert": {
          "name": "Motor RAG - Alta Tasa de Errores",
          "message": "La tasa de errores del Motor RAG ha superado el 5%",
          "frequency": "10s",
          "conditions": [
            {
              "query": {"queryType": "", "refId": "A"},
              "reducer": {"type": "last", "params": []},
              "evaluator": {"params": [5], "type": "gt"}
            }
          ],
          "executionErrorState": "alerting",
          "noDataState": "no_data",
          "for": "5m"
        }
      },
      
      {
        "id": 3,
        "title": "‚ö° Latencia P95 (ms)",
        "type": "stat",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(http_request_duration_seconds_bucket{job=\"ai-code-mentor\",endpoint=\"/api/generate-lesson\"}[5m])) * 1000",
            "legendFormat": "P95 Latency",
            "refId": "B"
          }
        ],
        "gridPos": {"h": 8, "w": 6, "x": 6, "y": 3},
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "textMode": "auto",
          "colorMode": "background",
          "graphMode": "area"
        },
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 1000},
                {"color": "red", "value": 1500}
              ]
            },
            "unit": "ms",
            "min": 0
          }
        },
        "alert": {
          "name": "Motor RAG - Latencia Alta P95",
          "message": "La latencia P95 del Motor RAG ha superado 1200ms",
          "frequency": "10s",
          "conditions": [
            {
              "query": {"queryType": "", "refId": "B"},
              "reducer": {"type": "last", "params": []},
              "evaluator": {"params": [1200], "type": "gt"}
            }
          ],
          "executionErrorState": "alerting",
          "noDataState": "no_data",
          "for": "2m"
        }
      },
      
      {
        "id": 4,
        "title": "üìà Invocaciones por Minuto",
        "type": "stat",
        "targets": [
          {
            "expr": "rate(http_requests_total{job=\"ai-code-mentor\",endpoint=\"/api/generate-lesson\"}[1m]) * 60",
            "legendFormat": "Requests/min",
            "refId": "C"
          }
        ],
        "gridPos": {"h": 8, "w": 6, "x": 12, "y": 3},
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "textMode": "auto",
          "colorMode": "background",
          "graphMode": "area"
        },
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "unit": "reqps",
            "min": 0
          }
        }
      },
      
      {
        "id": 5,
        "title": "üîÑ Estado del Fallback Legacy",
        "type": "stat",
        "targets": [
          {
            "expr": "rate(rag_fallback_total{job=\"ai-code-mentor\",type=\"legacy\"}[5m]) * 60",
            "legendFormat": "Legacy Fallback/min",
            "refId": "D"
          }
        ],
        "gridPos": {"h": 8, "w": 6, "x": 18, "y": 3},
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "textMode": "auto",
          "colorMode": "background"
        },
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 1},
                {"color": "red", "value": 5}
              ]
            },
            "unit": "reqps",
            "min": 0
          }
        },
        "alert": {
          "name": "Motor RAG - Uso Excesivo de Fallback",
          "message": "El Motor RAG est√° usando fallback legacy m√°s del 10% del tiempo",
          "frequency": "10s",
          "conditions": [
            {
              "query": {"queryType": "", "refId": "D"},
              "reducer": {"type": "last", "params": []},
              "evaluator": {"params": [5], "type": "gt"}
            }
          ],
          "executionErrorState": "alerting",
          "noDataState": "no_data",
          "for": "5m"
        }
      },
      
      {
        "id": 6,
        "title": "üìä Latencia Hist√≥rica - √öltimas 6 horas",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.50, rate(http_request_duration_seconds_bucket{job=\"ai-code-mentor\",endpoint=\"/api/generate-lesson\"}[5m])) * 1000",
            "legendFormat": "P50",
            "refId": "E"
          },
          {
            "expr": "histogram_quantile(0.95, rate(http_request_duration_seconds_bucket{job=\"ai-code-mentor\",endpoint=\"/api/generate-lesson\"}[5m])) * 1000",
            "legendFormat": "P95",
            "refId": "F"
          },
          {
            "expr": "histogram_quantile(0.99, rate(http_request_duration_seconds_bucket{job=\"ai-code-mentor\",endpoint=\"/api/generate-lesson\"}[5m])) * 1000",
            "legendFormat": "P99",
            "refId": "G"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 11},
        "xAxis": {"show": true},
        "yAxes": [
          {
            "label": "Latencia (ms)",
            "show": true,
            "min": 0
          }
        ],
        "legend": {
          "show": true,
          "values": true,
          "current": true,
          "alignAsTable": true
        },
        "tooltip": {"shared": true, "sort": 0, "value_type": "individual"}
      },
      
      {
        "id": 7,
        "title": "üéØ M√©tricas RAG Espec√≠ficas",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(rag_retrieve_sources_duration_seconds_sum[5m]) / rate(rag_retrieve_sources_duration_seconds_count[5m]) * 1000",
            "legendFormat": "retrieve_sources() avg ms",
            "refId": "H"
          },
          {
            "expr": "rate(rag_context_augmentation_duration_seconds_sum[5m]) / rate(rag_context_augmentation_duration_seconds_count[5m]) * 1000",
            "legendFormat": "Context Augmentation avg ms",
            "refId": "I"
          },
          {
            "expr": "rate(rag_cache_hits_total[5m]) / (rate(rag_cache_hits_total[5m]) + rate(rag_cache_misses_total[5m])) * 100",
            "legendFormat": "Cache Hit Rate %",
            "refId": "J"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 11},
        "yAxes": [
          {
            "label": "ms / %",
            "show": true,
            "min": 0
          }
        ],
        "legend": {
          "show": true,
          "values": true,
          "current": true,
          "alignAsTable": true
        }
      },
      
      {
        "id": 8,
        "title": "üìö Distribuci√≥n de Semanas Solicitadas",
        "type": "piechart",
        "targets": [
          {
            "expr": "sum by (week_id) (rate(rag_requests_by_week_total[1h]))",
            "legendFormat": "Semana {{week_id}}",
            "refId": "K"
          }
        ],
        "gridPos": {"h": 8, "w": 8, "x": 0, "y": 19},
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "pieType": "pie",
          "tooltip": {"mode": "single"},
          "legend": {
            "displayMode": "table",
            "placement": "right",
            "values": ["percent"]
          }
        }
      },
      
      {
        "id": 9,
        "title": "üî• Top 10 Semanas M√°s Utilizadas",
        "type": "table",
        "targets": [
          {
            "expr": "topk(10, sum by (week_id, week_title) (rate(rag_requests_by_week_total[24h])))",
            "legendFormat": "{{week_id}} - {{week_title}}",
            "refId": "L"
          }
        ],
        "gridPos": {"h": 8, "w": 8, "x": 8, "y": 19},
        "options": {
          "showHeader": true
        },
        "fieldConfig": {
          "defaults": {
            "custom": {
              "align": "auto",
              "displayMode": "auto"
            }
          }
        }
      },
      
      {
        "id": 10,
        "title": "‚ö° Status de Salud Sistema",
        "type": "stat",
        "targets": [
          {
            "expr": "up{job=\"ai-code-mentor\"}",
            "legendFormat": "Sistema Activo",
            "refId": "M"
          },
          {
            "expr": "rag_curriculum_loaded",
            "legendFormat": "Curriculum Cargado",
            "refId": "N"
          },
          {
            "expr": "rag_cache_healthy",
            "legendFormat": "Cache Saludable",
            "refId": "O"
          }
        ],
        "gridPos": {"h": 8, "w": 8, "x": 16, "y": 19},
        "options": {
          "colorMode": "background",
          "graphMode": "none",
          "justifyMode": "center"
        },
        "fieldConfig": {
          "defaults": {
            "mappings": [
              {
                "options": {
                  "0": {"text": "‚ùå OFFLINE", "color": "red"},
                  "1": {"text": "‚úÖ ONLINE", "color": "green"}
                },
                "type": "value"
              }
            ]
          }
        }
      }
    ],
    
    "time": {
      "from": "now-6h",
      "to": "now"
    },
    
    "timepicker": {
      "refresh_intervals": ["5s", "10s", "30s", "1m", "5m", "15m", "30m", "1h", "2h", "1d"]
    },
    
    "annotations": {
      "list": [
        {
          "name": "Deploys",
          "datasource": "-- Grafana --",
          "type": "dashboard",
          "iconColor": "rgba(0, 211, 255, 1)",
          "enable": true,
          "showIn": 0,
          "tags": ["deploy", "motor-rag"]
        }
      ]
    },
    
    "templating": {
      "list": [
        {
          "name": "instance",
          "type": "query",
          "query": "label_values(up{job=\"ai-code-mentor\"}, instance)",
          "refresh": 1,
          "includeAll": true,
          "multi": true,
          "allValue": ".*"
        }
      ]
    }
  }
}
